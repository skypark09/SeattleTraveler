package AirbnbSeattle.model;

import java.util.Date;

public class Reviews {
	
	protected int ReviewId;
    protected int UserId;
    protected String Username;
    protected Date Created;
    protected String Content;
    protected Long ListingId;

    public Reviews(int reviewId, int userId, String username, Date created, String content, Long listingId) {
		super();
		ReviewId = reviewId;
		UserId = userId;
		Username = username;
		Created = created;
		Content = content;
		ListingId = listingId;
	}
    
    public Reviews(int userId, String username, Date created, String content, Long listingId) {
		super();
		UserId = userId;
		Username = username;
		Created = created;
		Content = content;
		ListingId = listingId;
	}

	public int getReviewId() {
		return ReviewId;
	}

	public void setReviewId(int reviewId) {
		ReviewId = reviewId;
	}

	public int getUserId() {
		return UserId;
	}

	public void setUserId(int userId) {
		UserId = userId;
	}

	public String getUsername() {
		return Username;
	}

	public void setUsername(String username) {
		Username = username;
	}

	public Date getCreated() {
		return Created;
	}

	public void setCreated(Date created) {
		Created = created;
	}

	public String getContent() {
		return Content;
	}

	public void setContent(String content) {
		Content = content;
	}

	public Long getListingId() {
		return ListingId;
	}

	public void setListingId(Long listingId) {
		ListingId = listingId;
	}
    
    
}
